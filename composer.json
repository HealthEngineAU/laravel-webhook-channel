{
    "name": "healthengine/laravel-webhook-channel",
    "description": "Webhook Notifications driver",
    "homepage": "https://github.com/healthengineau/laravel-webhook-channel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com",
            "homepage": "http://www.marcelpociot.com"
        },
        {
            "name": "Jarryd Tilbrook",
            "email": "jrad.tilbrook@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": ">=5.5",
        "illuminate/support": ">=5.5"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*|~6.0",
        "orchestra/testbench": "3.3.x-dev|^3.5.0",
        "orchestra/database": "3.3.x-dev|^3.5.0"
    },
    "replace": {
        "laravel-notification-channels/webhook": "*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Webhook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Webhook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
